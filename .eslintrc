{
    "root": true,
    "parserOptions": {
        "ecmaVersion": "latest",
    },
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true,
    },
    "plugins": ["import", "prettier"],
    "extends": ["eslint:recommended", "plugin:import/errors", "plugin:prettier/recommended"],
    "rules": {
        "indent": ["warn", 4],
        "quotes": ["warn", "double"],
        "no-unused-vars": "warn",
    },
}
